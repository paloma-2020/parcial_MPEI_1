.global _main
_main:
	MOV     #0X0800, W0
	MOV     W0, TRISB
        ;BCLR    TRISB, #0xA
        ;BCLR    TRISB, #0XB
        MOV        #0X0000, W0
        MOV        W0, PORTB
_bucle:
	MOV     #0XFFFF, W0
	MOV     W0,LATB
	CALL    DELAY1S
	MOV     #0X0000, W0
	MOV    W0, LATB
	CALL    DELAY1S
	GOTO    _bucle
	
DELAY1S:
        MOV        #1000,W0
REPETIR:
        DEC        W0, W1
        MOV        W1, W0
        BRA        Z, RETORNAR
        REPEAT  #5000
        NOP
        GOTO    REPETIR
RETORNAR:
        RETURN 
        
.END


